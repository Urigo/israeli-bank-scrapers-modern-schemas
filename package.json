{
  "name": "modern-poalim-scraper",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node --experimental-json-modules dist/index.js",
    "dev": "tsc && concurrently \"tsc -w\" \"nodemon --experimental-specifier-resolution=node --experimental-json-modules dist/index.js\"",
    "build": "json2ts -i src/schemas -o dist/generatedTypes && tsc",
    "format": "prettier \"**/*.ts\" --write",
    "json2ts": "json2ts -i src/schemas -o src/generatedTypes",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "ajv": "8.6.3",
    "ajv-formats": "2.1.1",
    "inquirer": "8.1.5",
    "node-fetch": "3.0.0",
    "puppeteer": "10.2.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/inquirer": "8.1.2",
    "@types/node": "16.7.4",
    "@types/node-fetch": "3.0.2",
    "@types/uuid": "8.3.1",
    "concurrently": "6.2.1",
    "husky": "7.0.2",
    "json-schema-to-typescript": "10.1.5",
    "lint-staged": "11.1.2",
    "nodemon": "2.0.12",
    "prettier": "2.4.1",
    "typescript": "4.4.3"
  },
  "engines": {
    "node": "16.9.1"
  },
  "license": "MIT",
  "lint-staged": {
    "*.--write": "prettier --write"
  }
}
