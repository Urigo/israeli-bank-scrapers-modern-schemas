{
  "type": "module",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node --experimental-json-modules dist/index.js",
    "dev": "tsc && concurrently \"tsc -w\" \"nodemon --experimental-specifier-resolution=node --experimental-json-modules dist/index.js\"",
    "build": "tsc",
    "format": "prettier \"**/*.ts\" --write",
    "json-to-ts": "json2ts -i src/schemas -o generatedTypes"
  },
  "dependencies": {
    "ajv": "6.12.3",
    "dotenv": "8.2.0",
    "inquirer": "7.3.0",
    "lodash": "^4.17.19",
    "moment": "2.27.0",
    "node-fetch": "2.6.0",
    "puppeteer": "5.0.0",
    "uuid": "8.2.0"
  },
  "devDependencies": {
    "@types/dotenv": "8.2.0",
    "@types/inquirer": "6.5.0",
    "@types/lodash": "^4.14.157",
    "@types/node": "14.0.14",
    "@types/node-fetch": "2.5.7",
    "@types/puppeteer": "3.0.1",
    "@types/uuid": "8.0.0",
    "concurrently": "5.2.0",
    "json-schema-to-typescript": "9.1.1",
    "nodemon": "2.0.4",
    "prettier": "2.0.5",
    "typescript": "3.9.6"
  },
  "engines": {
    "node": "14.5.0"
  },
  "license": "MIT"
}
