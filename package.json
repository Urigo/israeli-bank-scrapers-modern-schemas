{
  "name": "modern-poalim-scraper",
  "version": "0.8.6",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node --experimental-json-modules dist/index.js",
    "dev": "tsc && concurrently \"tsc -w\" \"nodemon --experimental-specifier-resolution=node --experimental-json-modules dist/index.js\"",
    "build": "json2ts -i src/schemas -o dist/generatedTypes && tsc",
    "format": "prettier \"**/*.ts\" --write",
    "json2ts": "json2ts -i src/schemas -o src/generatedTypes",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "ajv": "8.12.0",
    "ajv-formats": "2.1.1",
    "inquirer": "9.2.10",
    "node-fetch": "3.3.2",
    "puppeteer": "21.0.1",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@types/inquirer": "9.0.3",
    "@types/node": "18.17.3",
    "@types/node-fetch": "3.0.3",
    "@types/uuid": "9.0.2",
    "concurrently": "8.2.0",
    "husky": "8.0.3",
    "json-schema-to-typescript": "13.0.2",
    "lint-staged": "13.3.0",
    "nodemon": "3.0.1",
    "prettier": "3.0.1",
    "typescript": "5.1.6"
  },
  "engines": {
    "node": "20.5.0"
  },
  "license": "MIT",
  "lint-staged": {
    "*.--write": "prettier --write"
  }
}
