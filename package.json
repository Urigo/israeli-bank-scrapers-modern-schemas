{
  "name": "modern-poalim-scraper",
  "version": "0.3.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node --experimental-json-modules dist/index.js",
    "dev": "tsc && concurrently \"tsc -w\" \"nodemon --experimental-specifier-resolution=node --experimental-json-modules dist/index.js\"",
    "build": "json2ts -i src/schemas -o dist/generatedTypes && tsc",
    "format": "prettier \"**/*.ts\" --write",
    "json2ts": "json2ts -i src/schemas -o src/generatedTypes",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "ajv": "8.11.0",
    "ajv-formats": "^2.1.1",
    "inquirer": "8.2.4",
    "node-fetch": "3.2.4",
    "puppeteer": "13.7.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/inquirer": "8.2.1",
    "@types/node": "17.0.29",
    "@types/node-fetch": "3.0.3",
    "@types/uuid": "8.3.4",
    "concurrently": "7.1.0",
    "husky": "7.0.4",
    "json-schema-to-typescript": "10.1.5",
    "lint-staged": "12.4.1",
    "nodemon": "2.0.15",
    "prettier": "2.6.2",
    "typescript": "4.6.3"
  },
  "engines": {
    "node": "18.0.0"
  },
  "license": "MIT",
  "lint-staged": {
    "*.--write": "prettier --write"
  }
}
