{
  "name": "modern-poalim-scraper",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node --experimental-json-modules dist/index.js",
    "dev": "tsc && concurrently \"tsc -w\" \"nodemon --experimental-specifier-resolution=node --experimental-json-modules dist/index.js\"",
    "build": "tsc",
    "format": "prettier \"**/*.ts\" --write",
    "json2ts": "json2ts -i src/schemas -o generatedTypes"
  },
  "dependencies": {
    "ajv": "7.1.1",
    "ajv-formats": "1.5.1",
    "inquirer": "7.3.3",
    "node-fetch": "2.6.1",
    "puppeteer": "7.1.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/inquirer": "7.3.1",
    "@types/node": "14.14.28",
    "@types/node-fetch": "2.5.8",
    "@types/uuid": "8.3.0",
    "concurrently": "5.3.0",
    "husky": "4.3.8",
    "json-schema-to-typescript": "10.1.3",
    "lint-staged": "10.5.4",
    "nodemon": "2.0.7",
    "prettier": "2.2.1",
    "typescript": "4.1.5"
  },
  "engines": {
    "node": "15.8.0"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.--write": "prettier --write"
  }
}